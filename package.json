{
  "name": "ionicons-cleaner",
  "version": "1.0.0",
  "description": "Remove unused ionicons from build output folder and from ngsw.json",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "run": "schematics .:ionicons-cleaner"
  },
  "keywords": [
    "schematics",
    "ionic",
    "ionicons",
    "angular"
  ],
  "author": "Elad Elram",
  "license": "MIT",
  "homepage": "https://github.com/elelad/ionicons-cleaner",
  "repository": "github:elelad/ionicons-cleaner",
  "schematics": "./src/collection.json",
  "dependencies": {
    "typescript": "~4.3.2",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.20.27",
    "@types/yargs": "^17.0.3",
    "jasmine": "^3.5.0"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "bin": {
    "ionicons-cleaner": "src/bin/clean-unused.js"
  }
}
