{
  "name": "ionicons-cleaner",
  "version": "0.0.5",
  "description": "Remove unused ionicons from build output folder and from ngsw.json",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "run": "schematics .:ionicons-cleaner"
  },
  "keywords": [
    "schematics",
    "ionic",
    "ionicons",
    "angular"
  ],
  "author": "Elad Elram",
  "license": "MIT",
  "homepage": "https://github.com/elelad/ionicons-cleaner",
  "repository": "github:elelad/ionicons-cleaner",
  "schematics": "./src/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^12.2.6",
    "@angular-devkit/schematics": "^12.2.6",
    "@angular-devkit/schematics-cli": "^12.2.6",
    "@schematics/angular": "^12.2.6",
    "typescript": "~4.3.2"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.8.0",
    "jasmine": "^3.5.0"
  }
}
