{
    "$schema": "http://json-schema.org/schema",
    "$id": "ioniconsTreeShaking",
    "title": "ionicons tree shaking schema",
    "type": "object",
    "description": "Remove not used icons",
    "properties": {
      "outputPath": {
        "type": "string",
        "description": "outputPath of the build"
      },
      "svgRelativePath": {
        "type": "string",
        "description": "Relative path of the svg folder from the outputPath",
        "default": "svg"
      }
    }
   }